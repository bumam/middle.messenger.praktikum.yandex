{
  "compileOnSave": true,
  "noImplicitAny": false,
  "noUnusedLocals": false,
  "noUnusedParameters": false,
  "sourceMap": false,
  "strictNullChecks": false,
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "declaration": false,
    "module": "commonjs",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
